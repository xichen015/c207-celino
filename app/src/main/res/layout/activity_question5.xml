<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EAFBFA"
    tools:context=".question5">

    <TextView
        android:id="@+id/q5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="149dp"
        android:layout_marginBottom="31dp"
        android:text="Question #5"
        android:textSize="34sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="26dp"
        android:text="Exact date Google was released?"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/answerG5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/q5" />

    <RadioGroup
        android:id="@+id/answerG5"
        android:layout_width="192dp"
        android:layout_height="0dp"
        android:layout_marginBottom="34dp"
        app:layout_constraintBottom_toTopOf="@+id/button6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7">

        <RadioButton
            android:id="@+id/a17"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="09 23 2008" />

        <RadioButton
            android:id="@+id/a18"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="05 16 2006" />

        <RadioButton
            android:id="@+id/a19"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="07 29 2007" />

        <RadioButton
            android:id="@+id/a20"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="06 15 2004" />
    </RadioGroup>

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="164dp"
        android:backgroundTint="#009688"
        android:text="Submit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/answerG5" />
</androidx.constraintlayout.widget.ConstraintLayout>